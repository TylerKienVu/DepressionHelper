<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" fx:id="journalsAnchorPane" prefHeight="680.0" prefWidth="812.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.JournalController">
   <children>
      <ListView fx:id="entriesListView" layoutX="49.0" layoutY="53.0" prefHeight="244.0" prefWidth="714.0" />
      <HBox alignment="CENTER_RIGHT" layoutX="569.0" layoutY="306.0" prefHeight="34.0" prefWidth="194.0">
         <children>
            <Button fx:id="newBtn" mnemonicParsing="false" onAction="#loadNewJournalEntry" prefHeight="25.0" prefWidth="41.0" text="New">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="editBtn" mnemonicParsing="false" onAction="#loadSelectedJournal" text="Edit">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#deleteSelectedJournal" text="Delete">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
